{
  "compilerOptions": {
    "jsx": "react",
    "module": "esnext",
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true
  },
  "jsx": "react-jsx",
  "include": ["src"]
}
